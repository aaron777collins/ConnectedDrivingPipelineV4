    "min_distance": 100,
    "max_distance": 200,
    "random_seed": 42
  },
  "ml": {
    "train_test_split": {
      "type": "random",
      "train_ratio": 0.8,
      "test_ratio": 0.2,
      "random_seed": 42
    },
    "classifiers": ["RandomForestClassifier", "DecisionTreeClassifier", "KNeighborsClassifier"]
  },
  "cache": {
    "enabled": true
  },
  "_special_features": {}
}
